      "min_account_balance": {"type": "numeric", "min": 0, "max": 10000000, "higher_is_better": true},
      "income_stability_score": {"type": "numeric", "min": 0, "max": 1, "higher_is_better": true},
      "income_variance_coefficient": {"type": "numeric", "min": 0, "max": 2, "higher_is_better": false},
      "itr_filed": {"type": "binary", "min": 0, "max": 1, "higher_is_better": true},
      "itr_income_declared": {"type": "numeric", "min": 0, "max": 50000000, "higher_is_better": true},
