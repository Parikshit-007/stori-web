      "location_tier": {"type": "categorical", "categories": ["tier1", "tier2", "tier3", "rural"], "higher_is_better": null},
      "education_level": {"type": "ordinal", "min": 0, "max": 5, "higher_is_better": true, "mapping": {"none": 0, "high_school": 1, "diploma": 2, "bachelors": 3, "masters": 4, "doctorate": 5}},
